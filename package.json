{
  "name": "looker-slackbot",
  "version": "0.0.12",
  "description": "Lookerbot for Slack",
  "main": "index.js",
  "scripts": {
    "start": "./node_modules/.bin/ts-node ./lib/index.ts",
    "dev": "./node_modules/.bin/nodemon --exec ./node_modules/.bin/ts-node --no-cache -- ./lib/index.ts",
    "test": "yarn test-ts && yarn lint",
    "test-ts": "./node_modules/.bin/mocha -r ./node_modules/ts-node/register test/**/test.ts",
    "lint": "./node_modules/.bin/tslint -c tslint.json 'lib/**/*.ts'",
    "lint-fix": "./node_modules/.bin/tslint --fix -c tslint.json 'lib/**/*.ts'"
  },
  "dependencies": {
    "@google-cloud/storage": "^0.8.0",
    "@types/node": "^7.0.18",
    "@types/request": "^0.0.43",
    "aws-sdk": "^2.54.0",
    "azure-storage": "~0.10.0",
    "botkit": "0.0.15",
    "dotenv": "^4.0.0",
    "fuzzysearch-js": "^0.2.0",
    "get-urls": "^5.0.0",
    "jsonfile": "^3.0.0",
    "request": "^2.66.0",
    "semver": "^5.1.0",
    "stream-buffers": "^3.0.1",
    "ts-node": "^3.0.2",
    "typescript": "^2.3.2",
    "underscore": "^1.8.3",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "mocha": "^2.4.5",
    "nodemon": "^1.11.0",
    "ts-lint": "^4.5.1"
  },
  "engines": {
    "node": "^6.10.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/looker/looker-slackbot"
  },
  "license": "MIT"
}
